{
  "files.exclude": {
    "bin/**": true
  },
  "cmake.configureOnOpen": true,
  "search.exclude": {
    "bin": true,
    "build/**": true
  },
  "monkeyC.jungleFiles": "jungle/type/app.jungle;jungle/dist/local.jungle;jungle/versioning/local.jungle;shared.jungle",
  "markdownlint.config": {
    "MD041": false,
    "MD034": false
  },
  "prettierMonkeyC.typeCheckLevel": "Strict",
  "monkeyC.typeCheckLevel": "Strict",
  "monkeyC.compilerOptions": "--build-stats 0 -r",
  "prettierMonkeyC.checkCompilerLookupRules": "OFF",
  "cSpell.customDictionaries": {
    "custom-dictionary-workspace": {
      "name": "custom-dictionary-workspace",
      "path": "${workspaceFolder:WatchApp}/.cspell/custom-dictionary-workspace.txt",
      "addWords": true,
      "scope": "workspace"
    }
  },
  "prettier.tabWidth": 4
}
